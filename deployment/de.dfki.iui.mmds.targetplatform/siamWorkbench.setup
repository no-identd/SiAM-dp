<?xml version="1.0" encoding="UTF-8"?>
<setup:Project
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:git="http://www.eclipse.org/oomph/setup/git/1.0"
    xmlns:jdt="http://www.eclipse.org/oomph/setup/jdt/1.0"
    xmlns:pde="http://www.eclipse.org/oomph/setup/pde/1.0"
    xmlns:predicates="http://www.eclipse.org/oomph/predicates/1.0"
    xmlns:projects="http://www.eclipse.org/oomph/setup/projects/1.0"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    xmlns:setup.p2="http://www.eclipse.org/oomph/setup/p2/1.0"
    xmlns:setup.targlets="http://www.eclipse.org/oomph/setup/targlets/1.0"
    xmlns:setup.workingsets="http://www.eclipse.org/oomph/setup/workingsets/1.0"
    xsi:schemaLocation="http://www.eclipse.org/oomph/setup/git/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Git.ecore http://www.eclipse.org/oomph/setup/jdt/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/JDT.ecore http://www.eclipse.org/oomph/setup/pde/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/PDE.ecore http://www.eclipse.org/oomph/predicates/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Predicates.ecore http://www.eclipse.org/oomph/setup/projects/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Projects.ecore http://www.eclipse.org/oomph/setup/targlets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupTarglets.ecore http://www.eclipse.org/oomph/setup/workingsets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupWorkingSets.ecore"
    name="siam.workbench"
    label="SiAM Workbench">
  <setupTask
      xsi:type="jdt:JRETask"
      version="JavaSE-1.8"
      location="${jre.location-1.8}">
    <description>Define the JRE needed to compile and run the Java projects of ${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="setup:EclipseIniTask"
      option="-Xmx"
      value="2048m"
      vm="true">
    <description>Set the heap space needed to work with the projects of ${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="setup.p2:P2Task">
    <requirement
        name="name.schedenig.eclipse.grepconsole.feature.group"/>
    <requirement
        name="biz.junginger.ExploreFS"/>
    <requirement
        name="org.eclipse.m2e.feature.feature.group"/>
    <requirement
        name="realignment.jd.ide.eclipse.feature.feature.group"/>
    <repository
        url="http://eclipse.schedenig.name/"/>
    <repository
        url="http://www.junginger.biz/eclipse/"/>
    <repository
        url="http://mchr3k-eclipse.appspot.com/"/>
    <description>Install the tools needed in the IDE to work with the source code for ${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      name="Preferences">
    <setupTask
        xsi:type="setup:PreferenceTask"
        key="/instance/name.schedenig.eclipse.grepconsole/expressions"
        value="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot; standalone=&quot;no&quot;?>&lt;folders xmlns=&quot;http://eclipse.musgit.com/grepconsole&quot;>&lt;style bold=&quot;false&quot; border=&quot;false&quot; foreground=&quot;7f7f7f&quot; id=&quot;539dd97b-36dc-4d5f-9fd2-14e94d9b015d&quot; italic=&quot;false&quot; name=&quot;Debug&quot; strikeout=&quot;false&quot; underline=&quot;false&quot;/>&lt;style background=&quot;ff0000&quot; bold=&quot;false&quot; border=&quot;false&quot; id=&quot;8720d322-7fd4-4bac-84cb-71a89591bbbb&quot; italic=&quot;false&quot; name=&quot;Error&quot; strikeout=&quot;false&quot; underline=&quot;false&quot;/>&lt;style background=&quot;a52a2a&quot; bold=&quot;true&quot; border=&quot;false&quot; foreground=&quot;ffff00&quot; id=&quot;79ade458-a1ae-498b-9d83-a6a9f469185f&quot; italic=&quot;false&quot; name=&quot;Fatal&quot; strikeout=&quot;false&quot; underline=&quot;false&quot;/>&lt;style background=&quot;ffd27f&quot; bold=&quot;false&quot; border=&quot;false&quot; id=&quot;2d9ece6e-0325-4d39-8579-4456851f259c&quot; italic=&quot;false&quot; name=&quot;Warn&quot; strikeout=&quot;false&quot; underline=&quot;false&quot;/>&lt;style bold=&quot;false&quot; border=&quot;false&quot; id=&quot;c83d6771-b9c0-4eb5-a27d-8a2166d3b13b&quot; italic=&quot;false&quot; strikeout=&quot;false&quot; underline=&quot;false&quot;/>&lt;folder enabled=&quot;true&quot; filter=&quot;true&quot; id=&quot;35882a8b-4b75-42c9-9f5d-f0f83f4432da&quot; notifications=&quot;true&quot; statistics=&quot;true&quot;>&lt;folder enabled=&quot;true&quot; id=&quot;75da4f67-40e8-45e0-8c75-8292c6f9e5db&quot; name=&quot;Log Output&quot;>&lt;item enabled=&quot;true&quot; expression=&quot;.*(\QDEBUG\E).*&quot; id=&quot;733554cb-6759-46e4-96b4-05b0ae9686e8&quot; name=&quot;Debug&quot;>&lt;group style=&quot;539dd97b-36dc-4d5f-9fd2-14e94d9b015d&quot;/>&lt;group/>&lt;/item>&lt;item enabled=&quot;true&quot; expression=&quot;.*(\QWARN\E).*&quot; id=&quot;805195bb-ae5d-4b68-b2c8-11e4688de116&quot; name=&quot;Warn&quot;>&lt;group style=&quot;2d9ece6e-0325-4d39-8579-4456851f259c&quot;/>&lt;group/>&lt;/item>&lt;item enabled=&quot;true&quot; expression=&quot;.*(\QERROR\E).*&quot; id=&quot;0f03e7f0-7082-462f-ae84-1883904b5301&quot; name=&quot;Error&quot;>&lt;group style=&quot;8720d322-7fd4-4bac-84cb-71a89591bbbb&quot;/>&lt;group/>&lt;/item>&lt;item enabled=&quot;true&quot; expression=&quot;.*(\QFATAL\E).*&quot; id=&quot;6da0cf2f-86fe-4054-9f74-522cb8137a72&quot; name=&quot;Fatal&quot;>&lt;group style=&quot;79ade458-a1ae-498b-9d83-a6a9f469185f&quot;/>&lt;group/>&lt;/item>&lt;/folder>&lt;/folder>&lt;/folders>"/>
    <setupTask
        xsi:type="setup:PreferenceTask"
        key="/instance/org.eclipse.core.resources/encoding"
        value="UTF-8"/>
  </setupTask>
  <stream name="ApplicationDevelopment">
    <setupTask
        xsi:type="setup.p2:P2Task">
      <requirement
          name="de.dfki.iui.mmds.model.feature.group"/>
      <requirement
          name="de.dfki.iui.mmds.sdk.feature.group"/>
      <requirement
          name="no.javatime.inplace.feature.feature.group"/>
      <repository
          url="http://madmacs.dfki.de/siam_dp/updateSite/3rd_party/"/>
      <repository
          url="http://madmacs.dfki.de/siam_dp/updateSite/mars/"/>
      <repository
          url="http://javatime.no/updates"/>
    </setupTask>
    <setupTask
        xsi:type="setup.targlets:TargletTask"
        id="siam">
      <targlet
          name="Platform"
          activeRepositoryList="SiAM"
          includeSources="false">
        <requirement
            name="org.eclipse.platform.feature.group"/>
        <requirement
            name="org.eclipse.equinox.cm"/>
        <requirement
            name="org.apache.commons.io"/>
        <requirement
            name="org.apache.commons.lang"/>
        <requirement
            name="org.apache.commons.beanutils"/>
        <requirement
            name="org.apache.commons.jexl"/>
        <requirement
            name="org.jdom"/>
        <requirement
            name="org.json"/>
        <requirement
            name="com.eclipsesource.jaxrs.connector.feature.feature.group"/>
        <requirement
            name="org.emfjson.feature.feature.group"/>
        <requirement
            name="org.eclipse.jetty.websocket.api"/>
        <requirement
            name="org.eclipse.jetty.websocket.server"/>
        <requirement
            name="de.dfki.iui.mmds.runtime.feature.group"/>
        <requirement
            name="org.apache.commons.collections"/>
        <requirement
            name="de.dfki.iui.mmds.model.feature.group"/>
        <requirement
            name="org.junit"/>
        <repositoryList
            name="SiAM">
          <repository
              url="http://download.eclipse.org/releases/mars"/>
          <repository
              url="http://download.eclipse.org/cbi/updates/license"/>
          <repository
              url="http://hstaudacher.github.io/osgi-jax-rs-connector"/>
          <repository
              url="http://madmacs.dfki.de/siam_dp/updateSite/mars/"/>
          <repository
              url="http://ghillairet.github.io/p2"/>
          <repository
              url="http://madmacs.dfki.de/siam_dp/updateSite/3rd_party"/>
        </repositoryList>
      </targlet>
    </setupTask>
  </stream>
  <stream name="SiAMPlatformDevelopment">
    <setupTask
        xsi:type="setup:CompoundTask"
        name="SiAMPlatform">
      <setupTask
          xsi:type="git:GitCloneTask"
          location="${workspace.location}/SiAM"
          remoteURI="git@www.forschungssoftware.de:dfki/SiAM-dp.git"
          checkoutBranch="master"/>
    </setupTask>
    <setupTask
        xsi:type="projects:ProjectsImportTask">
      <sourceLocator
          rootFolder="${workspace.location}/SiAM/platform">
        <predicate
            xsi:type="predicates:NotPredicate">
          <operand
              xsi:type="predicates:NamePredicate"
              pattern=".*(sip|mrcp|rtp|kapcom).*"/>
        </predicate>
      </sourceLocator>
      <sourceLocator
          rootFolder="${workspace.location}/SiAM/deployment"/>
      <sourceLocator
          rootFolder="${workspace.location}/SiAM/workbench/de.dfki.iui.mmds.sdk"/>
    </setupTask>
    <setupTask
        xsi:type="setup.workingsets:WorkingSetTask"
        id="buildWorkingSets">
      <workingSet
          name="Platform"
          id="platform">
        <predicate
            xsi:type="predicates:LocationPredicate"
            pattern=".*/platform/.*"/>
      </workingSet>
      <workingSet
          name="SDK"
          id="sdk">
        <predicate
            xsi:type="predicates:LocationPredicate"
            pattern=".*/workbench/.*"/>
      </workingSet>
      <workingSet
          name="Deployment"
          id="deployment">
        <predicate
            xsi:type="predicates:LocationPredicate"
            pattern=".*/deployment/.*"/>
      </workingSet>
      <workingSet
          name="Tests"
          id="tests">
        <predicate
            xsi:type="predicates:LocationPredicate"
            pattern=".*/tests/.*"/>
      </workingSet>
      <description>
        Build Working Sets

      </description>
    </setupTask>
    <setupTask
        xsi:type="pde:TargetPlatformTask"
        name="mmds.targetplatform"/>
  </stream>
  <logicalProjectContainer
      xsi:type="setup:ProjectCatalog"
      href="index:/org.eclipse.setup#//@projectCatalogs[name='org.eclipse']"/>
  <description>SiAM Workbench</description>
</setup:Project>
